$TTL 2d

$ORIGIN delegate.lan.

@      IN  SOA delegate.lan. ns.delegate.lan. (
             2022052603  ; serial
             12h     ; refresh
             15m     ; retry
             3w      ; expire
             2h )     ; minimum TTL
;
@ IN NS ns.delegate.lan.
ns IN A 127.0.0.1
delegate.lan. IN A 127.0.0.1
